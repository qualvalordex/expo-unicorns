{
  "expo": {
    "name": "Unicórnios",
    "owner": "qualvalordex",
    "slug": "ExpoUnicorns",
    "description": "Projeto desenvolvido com fins educacionais e para ajudar os amigos que não sabem inglês a jogarem o jogo Unstable Unicorns. Apresenta as traduções PT-BR das cartas do jogo.",
    "version": "0.0.3",
    "orientation": "portrait",
    "platforms":[
      "android"
    ],
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.osparsas.unicornios",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
